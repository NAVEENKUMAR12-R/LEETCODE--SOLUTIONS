class Solution {
public:
    int majorityElement(vector<int>& nums)
    {
      int n=nums.size();
     int a=n/2;
      //for(int i:nums)
      //{  
        //int c=count(nums.begin(),nums.end(),i);
        //if(c>a) return i;
    // }
     //return -1;
     sort(nums.begin(),nums.end());
    return nums[a];
    }
};
